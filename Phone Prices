#include <iostream>
using namespace std;
int main() 
{ int t,n;
cin>>t;
while(t--)
{ cin>>n;
int arr[n];
for(int i=0;i<n;i++)
{
    cin>>arr[i];
}
int min=0;
int temp=arr[0];
int count=1;
for(int i=1;i<=(n-5);i++)
{
    for(int j=i;j<5+i;j++)
    {
        min=arr[j];
        for(int k=j+1;k<=5+j;k++)
        {
            if(min>arr[k])
            {
                min=arr[k];
            }
            
        }
        if(min<temp)
        {  temp=min;
            count++;
        }
        
        
    }
}
 cout<<count-1<<endl;   
}

return 0;
}
