#include <iostream>
using namespace std;
int main() 
{ string s,s1;
  cin>>s;
  int t;
  cin>>t;
  while(t--)
  { int count=0;
    cin>>s1;
    for(int i=0;i<s1.length();i++)
    {
        for(int j=0;j<s.length();j++)
        {
            if(s1[i]==s[j])
            {
                count++;
                break;
            }
        }
    }
    if(count==s1.length())
    {
        cout<<"Yes"<<endl;
    }
    else
    {
        cout<<"No"<<endl;
    }
    
  
      
  }

return 0;
}
