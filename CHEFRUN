#include <iostream>
#include <cstdlib>
using namespace std;
int main() 
{int t,x1,x2,x3,v1,v2;
cin>>t;
while(t--)
{int d1,d2;
float t1,t2;
cin>>x1>>x2>>x3>>v1>>v2;
d1=abs(x3-x1);
d2=abs(x3-x2);
t1=float(d1)/v1;
t2=float(d2)/v2;
if(t1==t2)
{
    cout<<"Draw"<<endl;
}
else if(t1<t2)
{
    cout<<"Chef"<<endl;
}
else if(t1>t2)
{
    cout<<"Kefa"<<endl;
}
    
}

return 0;
}
