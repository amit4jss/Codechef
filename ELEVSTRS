#include <iostream>
#include<string.h>
#include<math.h>
using namespace std;
#include <algorithm>
#include <bits/stdc++.h>
int main() {

	int t;
	cin>>t;
	while(t--)
	{
	    float n,v1,v2;
	    cin>>n>>v1>>v2;
	    float t1,t2;
	    t1=(sqrt(2)*n)/v1;
	    t2=(2*n)/v2;
	    if(t1<=t2)
	        cout<<"Stairs"<<endl;
	   else
	        cout<<"Elevator"<<endl;
	     
	}
	return 0;
}
