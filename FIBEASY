#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
 
int fibonacci(int n)
{
    if((n==1)||(n==0))
    {
        return(n);
    }
    else
    {
        return(fibonacci(n-1)+fibonacci(n-2));
    }
}

bool is_IndexEven(int i) 
{
   static int k = 1;

   if(k == 1)
   {
       k++;
       return false;
   } 

   if(k++ % 2)
       return true;
   else 
       return false; 
 }
 
int main()
{  int t,n;
cin>>t;
while(t--)
{   cin>>n;
    vector<int> arr(n);
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int i=0;

    while(i<n)
    {
     arr[i]=(fibonacci(i)%10);
        i++;
    }
    
    while(arr.size()!=2)
    {
    
    arr.erase(remove_if(arr.begin(),arr.end(),is_IndexEven),arr.end());
     }
     
     for(int i=1;i<=arr.size();i++)
     {
         cout<<arr[i];
     }
}
    
    return 0;
}
