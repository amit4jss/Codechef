#include <iostream>
#include<vector>
using namespace std;

int main() 
{ int t;
long long int n,k;
cin>>t;
while(t--)
{cin>>n>>k;
int count=0;
vector<long long int> num(n);
for(int i=0;i<n;i++)
{
    cin>>num[i];
    if(num[i]>=k)
    {
        count++;
    }
}
if(count>0)
{
    cout<<"YES"<<endl;
}
else
{
    cout<<"NO"<<endl;
}
}
	return 0;
}
